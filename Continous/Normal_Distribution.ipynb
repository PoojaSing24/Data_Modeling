#area under curve--
def AUC(fun , a , b , n):
    # height of each small trepezium
    h = abs(b-a)/n
    area = 0 # intilization of area as 0
    
    # calculating and sum of all small-small trapeziums
    for i in range(n):
        A = a + i * h
        B = a + (i + 1) * h
        area += (fun(A) + fun(B)) * h / 2 

    return area

