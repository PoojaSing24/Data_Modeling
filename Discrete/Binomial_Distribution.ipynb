def calculate_tabulated_value(degrees_of_freedom):
    switch = {
        1: 3.841,
        2: 5.991,
        3: 7.815,
        4: 9.488,
        5: 11.070,
        6: 12.592,
        7: 14.067,
        8: 15.507,
        9: 16.919,
        10: 18.307,
        # Add more cases as needed
    }
    
    return switch.get(degrees_of_freedom, "Invalid degrees of freedom")


